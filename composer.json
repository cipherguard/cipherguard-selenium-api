{
  "name": "cipherguard/cipherguard-selenium-api",
  "description": "Cipherguard selenium testing helper endpoints",
  "type": "cakephp-plugin",
  "version": "4.5.0",
  "keywords": [
    "password",
    "cipherguard",
    "selenium"
  ],
  "homepage": "https://www.cipherguard.com",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Cipherguard Team",
      "homepage": "https://www.cipherguard.com/credits"
    }
  ],
  "support": {
    "bugs": "https://github.com/cipherguard/cipherguard/issues",
    "help": "https://www.cipherguard.com/help",
    "source": "https://github.com/cipherguard/cipherguard"
  },
  "require": {
    "cakephp/cakephp": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5 || ^9.3"
  },
  "autoload": {
    "psr-4": {
      "CipherguardSeleniumApi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CipherguardSeleniumApi\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
